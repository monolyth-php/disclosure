{
    "name": "monomelodies/disclosure",
    "description": "PHP5 dependency injection framework",
    "license": "MIT",
    "keywords": ["php","dependency","injection"],
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "monomelodies/gentry": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Disclosure\\": "src",
            "Psr\\Container\\": "psr"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Disclosure\\Test\\": "tests",
            "Demo\\": "demo"
        }
    }
}    
