{
    "name": "monolyth/disclosure",
    "description": "Dependency injection for the Monolyth unframework",
    "license": "MIT",
    "keywords": ["monolyth","php","dependency","injection"],
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "gentry/gentry": "^0.13.3"
    },
    "autoload": {
        "psr-4": {
            "Monolyth\\Disclosure\\": "src",
            "Psr\\Container\\": "psr"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Monolyth\\Disclosure\\Test\\": "tests",
            "Monolyth\\Disclosure\\Demo\\": "demo"
        }
    }
}    
